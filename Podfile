# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'NeuralBikes' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'ReactiveSwift'
  pod 'RxSwift'
  pod 'ReactiveCocoa'
  pod 'SwiftLint'

  # Pods for Bicis
  target 'NeuralBikesUITests' do
    # Pods for testing
    use_frameworks!
    pod 'ReactiveSwift'
  pod 'RxSwift'
    pod 'ReactiveCocoa'
  end

end
